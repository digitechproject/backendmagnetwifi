{
  "version": 2,
  "name": "lead_magnet_server",
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/index.php",
      "dest": "/api/index.php"
    },
    {
      "src": "/api/formulaire.php",
      "dest": "/api/formulaire.php"
    },
    {
      "src": "/api/traitement.php",
      "dest": "/api/traitement.php"
    },
    {
      "src": "/api/succes.php",
      "dest": "/api/succes.php"
    }
  ]
}
