{
    "version": 2,
    "builds": [
      {
        "src": "Dockerfile",
        "use": "@vercel/docker"
      }
    ],
    "routes": [
      { "src": "/api/index", "dest": "/api/index.php" },
      { "src": "/api/formulaire", "dest": "/api/formulaire.php" },
      { "src": "/api/traitement", "dest": "/api/traitement.php" },
      { "src": "/api/succes", "dest": "/api/succes.php" },
      { "src": "/assets/(.*)", "dest": "/assets/$1" }
    ]
  }
  